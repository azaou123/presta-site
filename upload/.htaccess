# Apache 2.2
<IfModule !mod_authz_core.c>
    <FilesMatch "^[0-9a-f]{40}(_small)?$">
        Order deny,allow
        Deny from all
    </FilesMatch>
</IfModule>

# Apache 2.4
<IfModule mod_authz_core.c>
    <FilesMatch "^[0-9a-f]{40}(_small)?$">
        Require all denied
    </FilesMatch>
</IfModule>
